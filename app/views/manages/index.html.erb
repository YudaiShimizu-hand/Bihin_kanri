<%# 検索機能 %>
<table>
<tr>
    <th>備品名</th>
    <th>収納場所</th>
    <th>数量</th>
    <th></th>
</tr>
<% @manages.each do |m| %>
<tr>
    <td><%= m.name %></td>
    <td><%= m.place %></td>
    <td><%= m.number %></td>
    <td>
        <%= link_to "変更", edit_manage_path(m.id) %>
        <%= link_to "削除", manage_path(m.id), method: :delete %>
    </td>
<tr>
<% end %>
</table>